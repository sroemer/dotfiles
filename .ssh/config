Host uberspace
    Hostname arcturus.uberspace.de
    User senvang
    IdentityFile ~/.ssh/uberspace_ed25519
    AddKeysToAgent yes

Host socks_uberspace
    DynamicForward 58080
    Hostname arcturus.uberspace.de
    User senvang
    IdentityFile ~/.ssh/uberspace_ed25519
    AddKeysToAgent yes

Host github
    Hostname github.com
    User git
    IdentityFile ~/.ssh/github_ed25519
    AddKeysToAgent yes
